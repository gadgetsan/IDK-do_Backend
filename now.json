{
    "name": "IDK-do_Backend",
    "version": 2,
    "builds": [
        {
            "src": "**/*.js",
            "use": "@now/node-server",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
}
